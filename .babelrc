{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 7",
            "ie >= 8"
          ]
        }
      }
    ]
  ],
  "ignore": [
    "node_modules/",
    "dist/"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
