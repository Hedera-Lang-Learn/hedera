{
  "name": "hedera",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hedera-Lang-Learn/hedera"
  },
  "scripts": {
    "start": "HOT_RELOAD=1 webpack-dev-server --mode development",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "eslint --ext .js,.vue --fix .",
    "build": "NODE_ENV=production webpack --mode production",
    "test:unit": "jest"
  },
  "private": true,
  "browserslist": "last 2 versions",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/vue-fontawesome": "^2.0.0",
    "@vuejs-community/vue-filter-date-format": "^1.6.3",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.2",
    "clipboard": "^2.0.6",
    "lodash.debounce": "^4.0.8",
    "vue": "^2.6.12",
    "vue-keybindings": "^1.0.0",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/runtime": "^7.14.6",
    "@sentry/webpack-plugin": "^1.12.1",
    "@vue/test-utils": "^1.1.0",
    "autoprefixer": "^10.2.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.1.0",
    "browserslist": "^4.14.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "css-hot-loader": "^1.4.3",
    "css-loader": "^4.3.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.1.0",
    "imports-loader": "^1.1.0",
    "jest": "^27.0.6",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.15",
    "postcss-loader": "^4.3.0",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "vue-eslint-parser": "^7.1.0",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-bundle-tracker": "^1.0.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "jest": true
    },
    "extends": [
      "airbnb-base",
      "plugin:vue/essential"
    ],
    "rules": {
      "max-len": 0,
      "no-console": 0,
      "no-param-reassign": [
        "error",
        {
          "props": true,
          "ignorePropertyModificationsFor": [
            "state",
            "acc",
            "e",
            "ctx",
            "req",
            "request",
            "res",
            "response",
            "map",
            "$scope"
          ]
        }
      ],
      "vue/script-indent": [
        "error",
        2,
        {
          "baseIndent": 1,
          "switchCase": 1
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "*.vue"
        ],
        "rules": {
          "indent": "off"
        }
      }
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "vue"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      ".*\\.(js)$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ]
  }
}
